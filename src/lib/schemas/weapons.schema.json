    },
    "ResoTriggers": {
      "enum": [
        "alt",
        "atk",
        "bene",
        "flame",
        "fort",
        "frost",
        "phys",
        "volt"
      ],
      "type": "string"
    },
    "EffectsIds": {
      "enum": [
        "grayfox-a3-buff",
        "grayfox-a3-debuff",
        "grayfox-a6",
        "grayfox-skill",
        "nanyin-a5",
        "nanyin-a6",
        "nanyin-inhabit",
        "nola-a3",
        "nola-a6",
        "nola-frost-skill"
      ],
      "type": "string"
    },
    "ResoEffectsIds": {
      "enum": [
        "atk-reso",
        "atk-reso-teamplay",
        "bene-reso",
        "fiona-reso",
        "flame-reso",
        "fort-reso",
        "frost-reso",
        "nanyin-reso",
        "phys-reso",
        "volt-reso"
