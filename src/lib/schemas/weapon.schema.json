{
  "type": "object",
  "properties": {
    "id": {
      "$ref": "#/definitions/WeaponsIds"
    },
    "name": {
      "type": "string"
    },
    "base_stat": {
      "$ref": "#/definitions/BaseStatType"
    },
    "resonances": {
      "type": "array",
      "items": [
        {
          "$ref": "#/definitions/ResoElement"
        },
        {
          "$ref": "#/definitions/ResoRole"
        }
      ],
      "minItems": 2,
      "maxItems": 2
    },
    "onfieldness": {
      "type": "number"
    },
    "effects": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/WeaponEffectsIds"
      }
    },
    "reso_effects": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ResoEffectsIds"
      }
    },
    "settings": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "type": "object",
            "properties": {
              "choices": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/WeaponSettingsIds"
                }
              },
              "default": {
                "$ref": "#/definitions/WeaponSettingsIds"
              },
              "assignment": {
                "$ref": "#/definitions/SettingAssignment"
              },
              "type": {
                "type": "string",
                "const": "skill"
              }
            },
            "additionalProperties": false,
            "required": [
              "assignment",
              "choices",
              "default",
              "type"
            ]
          },
          {
            "type": "object",
            "properties": {
              "choices": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/WeaponSettingsIds"
                }
              },
              "default": {
                "$ref": "#/definitions/WeaponSettingsIds"
              },
              "assignment": {
                "$ref": "#/definitions/SettingAssignment"
              },
              "type": {
                "type": "string",
                "const": "element"
              }
            },
            "additionalProperties": false,
            "required": [
              "assignment",
              "choices",
              "default",
              "type"
            ]
          }
        ]
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "base_stat",
    "effects",
    "id",
    "name",
    "resonances"
  ],
  "definitions": {
    "WeaponsIds": {
      "enum": [
        "anka",
        "antoria",
        "asuka",
        "asurada",
        "brevey",
        "claudiax",
        "grayfox",
        "merylx",
        "nanyin",
        "nola",
        "none",
        "roslyn",
        "voidpiercer",
        "yanuo"
      ],
      "type": "string"
    },
    "BaseStatType": {
      "description": "BaseStatType naming convetion:\n  [third stat]-[atk value]\n  third stat: crit, res\n  atk value: low, med, high",
      "enum": [
        "crit-high",
        "crit-low",
        "crit-med",
        "res-high",
        "res-low",
        "res-med"
      ],
      "type": "string"
    },
    "ResoElement": {
      "enum": [
        "alt",
        "flame",
        "flame-phys",
        "frost",
        "frost-volt",
        "none",
        "phys",
        "phys-flame",
        "volt",
        "volt-frost"
      ],
      "type": "string"
    },
    "ResoRole": {
      "enum": [
        "atk",
        "bene",
        "fort",
        "none"
      ],
      "type": "string"
    },
    "WeaponEffectsIds": {
      "enum": [
        "anka-a3",
        "anka-a6-skill",
        "anka-skill",
        "antoria-a3",
        "antoria-a6-skill",
        "antoria-skill",
        "asuka-a6",
        "asuka-a6-fort",
        "asuka-skill",
        "asurada-a6-skill",
        "asurada-skill",
        "brevey-a3-nonbene",
        "brevey-a6",
        "brevey-a6-link",
        "brevey-link",
        "brevey-skill",
        "brevey-skill-offhand",
        "claudiax-a6-skill",
        "claudiax-skill",
        "grayfox-a3-buff",
        "grayfox-a3-debuff",
        "grayfox-a6",
        "grayfox-skill",
        "merylx-a5",
        "merylx-discharge",
        "merylx-skill-fort",
        "nanyin-a5",
        "nanyin-a6",
        "nanyin-inhabit",
        "nola-a3",
        "nola-a6",
        "nola-frost-skill",
        "roslyn-a6",
        "voidpiercer-a3",
        "voidpiercer-a6",
        "voidpiercer-starfall-flame",
        "voidpiercer-starfall-frost",
        "voidpiercer-starfall-phys",
        "voidpiercer-starfall-volt",
        "yanuo-a1",
        "yanuo-a6-skill",
        "yanuo-skill"
      ],
      "type": "string"
    },
    "ResoEffectsIds": {
      "enum": [
        "armor-dissolve",
        "armor-dissolve-teamplay",
        "atk-reso",
        "atk-reso-teamplay",
        "bene-reso",
        "bene-reso-teamplay",
        "fiona-reso",
        "flame-reso",
        "force-impact",
        "force-impact-teamplay",
        "fort-reso",
        "frost-reso",
        "nanyin-reso",
        "phys-reso",
        "volt-reso"
      ],
      "type": "string"
    },
    "WeaponSettingsIds": {
      "enum": [
        "nola-alt",
        "nola-flame",
        "nola-frost",
        "nola-phys",
        "nola-volt",
        "voidpiercer-alt",
        "voidpiercer-flame",
        "voidpiercer-frost",
        "voidpiercer-phys",
        "voidpiercer-pursuit",
        "voidpiercer-starfall",
        "voidpiercer-volt"
      ],
      "type": "string"
    },
    "SettingAssignment": {
      "enum": [
        "manual",
        "voidpiercer"
      ],
      "type": "string"
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}